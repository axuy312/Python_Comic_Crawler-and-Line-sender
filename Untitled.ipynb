    "#! pip install flask\n",
    "from flask import Flask,jsonify,request\n",
    "app=Flask(__name__)\n",
    "\n",
    "@app.route(\"/callback\")\n",
    "def callback():\n",
    "    return jsonify({'data' : request.url})\n",
    "\n",
    "if __name__==\"__main__\":\n",
    "        app.run()"
